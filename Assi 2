using System;

public class While

            {

                public static void Main()

                       {

                                   int i;

                                   int n;

                       Console.WriteLine("Enter A Number:- ");

                                   n = Convert.ToInt32(Console.ReadLine());

                              for(i=1;i<=n;i++)

                                   {

                                   Console.WriteLine("Teach One,Each One,Tree One");

                                              }

                       Console.ReadLine();

                                   }

            }

